<templateSet group="Other">
  <template name="tstamp" value="$timestamp$" toReformat="false" toShortenFQNames="true">
    <variable name="timestamp" expression="groovyScript(&quot;(int) (System.currentTimeMillis() / 1000L);&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>